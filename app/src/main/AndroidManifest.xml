<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.mariajane.When_Reading">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name="org.mariajane.When_Reading.GenreListActivity"></activity>
        <activity android:name="org.mariajane.When_Reading.GenreActivity" />
        <activity android:name="org.mariajane.When_Reading.HowLongToReadActivity" />
        <activity android:name="org.mariajane.When_Reading.DisplayActivity" />
        <activity android:name="org.mariajane.When_Reading.StatsActivity" />
        <activity android:name="org.mariajane.When_Reading.BookDetailActivity" />
        <activity android:name="org.mariajane.When_Reading.BookListActivity" />
        <activity android:name="org.mariajane.When_Reading.AddActivity" />
        <activity android:name="org.mariajane.When_Reading.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>